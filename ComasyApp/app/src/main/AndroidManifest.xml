<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.comasyapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TakeRefrigeratorPictureActivity"></activity>
        <activity android:name=".ViewRefrigeratorPictureActivity" />
        <activity android:name=".RegistrationClickActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".ColumnActivity" />
        <activity android:name=".RecipeSearchActivity" />
        <activity android:name=".RecipeSearchResultActivity" />
        <activity android:name=".MemoActivity" />
        <activity android:name=".MemoTextActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".TermsOfServiceActivity" />
        <activity android:name=".MemberRegistrationConfirmActivity" />
        <activity android:name=".MemberRegistrationFormActivity" />
        <activity android:name=".MemberRegistrationActivity" />
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Launcher"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>